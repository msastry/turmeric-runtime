#set( $symbol_pound = '#' )
#set( $symbol_dollar = '$' )
#set( $symbol_escape = '\' )
<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <client-config>
        <service-interface-class-name>org.ebayopensource.turmeric.example.v1.services.echoservice.${rootArtifactId}</service-interface-class-name>
        <service-location>http://localhost:8080/echoservice-example/${rootArtifactId}</service-location>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="HTTP11"/>
                <request-data-binding>XML</request-data-binding>
                <response-data-binding>XML</response-data-binding>
                <consumer-id>${artifactId}</consumer-id>
            </invocation-options>
        </client-instance-config>
    </client-config>
</client-config-list>
