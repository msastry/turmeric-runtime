 <project>
  <groupId>org.ebayopensource.test</groupId>
  <artifactId>test1</artifactId>
  <version>1-SNAPSHOT</version>
  <build>
    <plugins>
      <plugin>
        <groupId>org.ebayopensource.turmeric.plugins</groupId>
        <artifactId>turmeric-maven-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>gen-interface-wsdl</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <verbose>true</verbose>
          <serviceName>RepositoryService</serviceName>
          <adminName>RepositoryService</adminName>
          <packageName>org.ebayopensource.test.service</packageName>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>

