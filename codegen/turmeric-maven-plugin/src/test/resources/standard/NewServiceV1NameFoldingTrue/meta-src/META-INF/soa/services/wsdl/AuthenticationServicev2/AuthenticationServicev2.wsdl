				<xs:sequence>
                    			<xs:element minOccurs="0" maxOccurs="unbounded" name="credential" type="tns:CredentialType"/>
				</xs:sequence>
			    </xs:extension>
			</xs:complexContent>
            	</xs:complexType>
            	
            <xs:complexType name="KeyValuePairType">
