 <project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.ebay.soa.typelib</groupId>
  <artifactId>sampleTypeLibrary</artifactId>
  <version>1.0.0</version>
  <dependencies>
    <dependency>
      <groupId>com.ebay.soa</groupId>
      <artifactId>SOAClient</artifactId>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.ebayopensource.turmeric.plugins</groupId>
        <artifactId>turmeric-maven-plugin</artifactId>
        <executions>
          <execution>
            <goals>
              <goal>gen-typelibrary</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <verbose>true</verbose>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
