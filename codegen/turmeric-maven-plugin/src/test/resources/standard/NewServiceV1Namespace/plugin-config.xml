<project xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd" xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.ebayopensource.turmeric.tests</groupId>
  <artifactId>NewServiceV1</artifactId>
  <version>1.0.0</version>
  <name>NewServiceV1</name>
  <properties>
    <implProjectName>NewServiceV1Impl</implProjectName>
  </properties>
  <dependencies>
    <dependency>
      <groupId>org.ebayopensource.turmeric.runtime</groupId>
      <artifactId>soa-client</artifactId>
      <version>1.0.0.0-SNAPSHOT</version>
    </dependency>
    <dependency>
      <groupId>org.ebayopensource.turmeric.runtime.typelib</groupId>
      <artifactId>common-type-library</artifactId>
      <version>1.0.0.0-SNAPSHOT</version>
    </dependency>
  </dependencies>
  <build>
    <plugins>
      <plugin>
        <groupId>org.ebayopensource.turmeric.maven</groupId>
        <artifactId>turmeric-maven-plugin</artifactId>
        <version>1.0.0.0-SNAPSHOT</version>
        <executions>
          <execution>
            <goals>
              <goal>gen-interface-wsdl</goal>
            </goals>
          </execution>
        </executions>
        <configuration>
          <verbose>true</verbose>
          <enableNamespaceFolding>true</enableNamespaceFolding>
          <sharedConsumer>true</sharedConsumer>
          <sharedConsumerPackage>org.ebayopensource.turmeric.sample_namespacepart.v1.services.newservice.gen</sharedConsumerPackage>
          <packageName>org.ebayopensource.turmeric.sample_namespacepart.v1.services.newservice</packageName>
          <className>NewServiceV1</className>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
