<service-config group="CommonSOAServiceGroup" xmlns="http://www.ebay.com/soaframework/common/config">
    <service-impl-class-name>org.ebayopensource.turmeric.repository.v1.services.repositoryservice.impl.TurmericRSV1Impl</service-impl-class-name>
    <service-interface-class-name>org.ebayopensource.turmeric.repository.v1.services.repositoryservice.impl.TurmericRSV1</service-interface-class-name>
    <service-instance-config>
        <protocol-processor version="1.1" name="SOAP11">
            <indicator>
                <transport-header name="X-EBAY-SOA-MESSAGE-PROTOCOL">SOAP11</transport-header>
            </indicator>
            <class-name>org.ebayopensource.turmeric.runtime.spf.impl.protocolprocessor.soap.ServerSOAPProtocolProcessor</class-name>
        </protocol-processor>
        <protocol-processor version="1.2" name="SOAP12">
            <indicator>
                <transport-header name="X-EBAY-SOA-MESSAGE-PROTOCOL">SOAP12</transport-header>
            </indicator>
            <class-name>org.ebayopensource.turmeric.runtime.spf.impl.protocolprocessor.soap.ServerSOAPProtocolProcessor</class-name>
        </protocol-processor>
    </service-instance-config>
</service-config>
