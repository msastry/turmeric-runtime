<client-config-groups>
	<client-group name="@@GROUP_NAME@@">
		<client-config>
			<pipeline-config>
				<request-handlers>
					<handler name="MessageContextHandler" presence="Replaceable">
						<class-name>@@MessageContextHandler@@</class-name>
					</handler>
				</request-handlers>
				<logging-handler>
					<class-name>@@ClientLoggingHandler@@</class-name>
				</logging-handler>
			</pipeline-config>
		</client-config>
	</client-group>
</client-config-groups>

