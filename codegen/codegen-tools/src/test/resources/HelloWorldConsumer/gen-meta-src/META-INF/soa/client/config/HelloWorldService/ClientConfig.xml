<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <client-config service-name="{http://www.ebayopensource.org/turmeric/common/v1/services}HelloWorldService">
        <service-interface-class-name>org.ebayopensource.qaservices.helloworld.intf.IHelloWorld</service-interface-class-name>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="LOCAL"/>
                <request-data-binding>XML</request-data-binding>
                <response-data-binding>XML</response-data-binding>
                <invocation-use-case>HelloWorldServiceClient</invocation-use-case>
            </invocation-options>
        </client-instance-config>
    </client-config>
</client-config-list>
