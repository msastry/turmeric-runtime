<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<service enable-namespace-folding="false" name="HelloWorldService" xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <package-map>
        <package xml-namespace="http://www.ebayopensource.org/turmeric/common/v1/services" name="org.ebayopensource.turmeric.tools.codegen"/>
        <package xml-namespace="http://www.ebayopensource.org/turmeric/common/v1/types" name="org.ebayopensource.turmeric.common.v1.types"/>
        <package xml-namespace="http://www.w3.org/2001/XMLSchema" name="java.lang"/>
    </package-map>
    <operation-list>
        <operation methodName="helloWorld" name="helloWorld">
            <request-message>
                <java-type-name>java.lang.String</java-type-name>
                <xml-type-name>string</xml-type-name>
                <xml-element-name>helloWorld</xml-element-name>
                <has-attachment>false</has-attachment>
            </request-message>
            <response-message>
                <java-type-name>java.lang.String</java-type-name>
                <xml-type-name>string</xml-type-name>
                <xml-element-name>helloWorld</xml-element-name>
                <has-attachment>false</has-attachment>
            </response-message>
            <error-message>
                <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorMessage</java-type-name>
                <xml-type-name>ErrorMessage</xml-type-name>
                <xml-element-name>{http://www.ebayopensource.org/turmeric/common/v1/types}errorMessage</xml-element-name>
            </error-message>
        </operation>
    </operation-list>
    <java-type-list/>
</service>
