            <xs:element name="CSUpdateMACActivityAddAttachmentsRequest" type="ns:CSUpdateMACActivityAddAttachmentsRequestType"/>
            	<xs:complexType name="CSUpdateMACActivityAddAttachmentsRequestType">
		<xs:annotation>
			<xs:documentation>
				Definition for adding a MAC attachment to an activity
			</xs:documentation>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ns:AbstractRequestType">
				<xs:sequence>
					<xs:element name="User" type="ns:CSUserIdType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								UserID, UserOracleID or EmailAddress
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ActivityID" type="xs:long" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								MAC Activity ID
							</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Attachment" type="ns:MACAttachementFileType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>
								Details of the attachment (name,size, and binary data (base64enocded)
							</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
            <xs:element name="CSUpdateMACActivityAddAttachmentsResponse" type="ns:CSUpdateMACActivityAddAttachmentsResponseType"/>
