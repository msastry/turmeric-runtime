                <s:element maxOccurs="1" minOccurs="0" name="Name" type="s:string"/>
                <s:element maxOccurs="unbounded" minOccurs="0" name="Value">
                  <s:complexType>
                    <s:sequence>
                      <s:element maxOccurs="1" minOccurs="0" name="Label" type="s:string"/>
                      <s:element maxOccurs="1" minOccurs="0" name="ValueCode" type="s:string"/>
                    </s:sequence>
                  </s:complexType>
                </s:element>
                <s:element maxOccurs="1" minOccurs="0" name="InputType" type="s:string"/>
              </s:sequence>
            </s:complexType>
          </s:element>
        </s:sequence>
      </s:complexType>
      <s:complexType name="ArrayOfString">
        <s:sequence>
