<service enable-namespace-folding="true" name="WrongServiceName" xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <package-map>
        <package xml-namespace="http://www.ebay.com/marketplace/advertising/v1/services" name="org.ebayopensource.turmeric.common.v1.types"/>
        <package xml-namespace="http://www.w3.org/2001/XMLSchema" name="java.lang"/>
    </package-map>
    <operation-list>
        <operation name="echoString">
            <request-message>
                <java-type-name>java.lang.String</java-type-name>
                <xml-type-name>string</xml-type-name>
                <xml-element-name>Message</xml-element-name>
            </request-message>
            <response-message>
                <java-type-name>java.lang.String</java-type-name>
                <xml-type-name>string</xml-type-name>
                <xml-element-name>Message</xml-element-name>
            </response-message>
            <error-message>
                <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorMessage</java-type-name>
                <xml-type-name>ErrorMessage</xml-type-name>
                <xml-element-name>ErrorMessage</xml-element-name>
            </error-message>
        </operation>
    </operation-list>
    <java-type-list>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.AckValue</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.BaseRequest</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.BaseResponse</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorCategory</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorData</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorMessage</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorParameter</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ErrorSeverity</java-type-name>
        <java-type-name>org.ebayopensource.turmeric.common.v1.types.ExtensionType</java-type-name>
    </java-type-list>
</service>
