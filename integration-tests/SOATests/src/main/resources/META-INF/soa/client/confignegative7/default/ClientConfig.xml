<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
<client-config service-name="{http://www.ebayopensource.org/turmeric/common/config}test1" group="SOAWebServiceClient" xmlns="http://www.ebayopensource.org/turmeric/common/config">
<service-interface-class-name>org.ebayopensource.turmeric.runtime.tests.service1.sample.services.message.Test1Service</service-interface-class-name>
<service-location>http://localhost:9091/ws/spf</service-location>
<wsdl-location>webservices/latest/eBaySvc.wsdl</wsdl-location>
<client-instance-config>
	<invocation-options>
		<preferred-transport name="Local"/>
		<G11N-options>
			<preferred-encoding>CP1252</preferred-encoding>
			<preferred-locale>en-CA</preferred-locale>
		</G11N-options>
		<request-data-binding>XML</request-data-binding>

	</invocation-options>
	<pipeline-config>
		<request-handlers>
				<handler name="Logging"/>
			<chain name="NoSuchChain">
				<!-- handler name="AffiliateTracking">
					<class-name>com.ebay.myhandlers.affiliatetrackinghandler</class-name>
				</handler -->
			</chain>
			<chain name="AppSpecific2">
				<!-- handler name="DoSomething1">
					<class-name>com.ebay.myhandlers.dosomething1handler</class-name>
				</handler>
				<handler name="DoSomething2">
					<class-name>com.ebay.myhandlers.dosomething1handler</class-name>
				</handler-->
			</chain>
		</request-handlers>
	</pipeline-config>
</client-instance-config>
</client-config>
</client-config-list>
