<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <client-config
        service-name="{http://www.ebayopensource.org/turmeric/common/v1/types}consumerid2"
        group="SOAWebServiceClient-consumerid"
        xmlns="http://www.ebayopensource.org/turmeric/common/config">
        <service-interface-class-name>
            org.ebayopensource.turmeric.runtime.tests.service1.sample.services.message.Test1Service
        </service-interface-class-name>
        <service-location>
            http://localhost:9091/ws/spf
        </service-location>
        <wsdl-location>webservices/latest/eBaySvc.wsdl</wsdl-location>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="Local" />
                <request-data-binding>XML</request-data-binding>
                <consumer-id>testConsumerId</consumer-id>
            </invocation-options>
            <pipeline-config>
	    	<request-handlers>
	    		<chain name="clienthandlers">
	    			<!-- placeholder chain for service client instances to configure client specific handlers -->
	    			<handler name="ClientConsumerIdTestHandler" presence="Mandatory" run-on-error="true">
	    				<class-name>org.ebayopensource.turmeric.runtime.tests.config.ClientConsumerIdTestHandler</class-name>
	                        </handler>
	    		</chain>					
	    	</request-handlers>
	     </pipeline-config>
        </client-instance-config>
    </client-config>
</client-config-list>
