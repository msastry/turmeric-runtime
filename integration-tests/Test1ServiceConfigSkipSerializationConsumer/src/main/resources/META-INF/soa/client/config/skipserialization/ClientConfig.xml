<client-config-list xmlns="http://www.ebayopensource.org/turmeric/common/config">
    <client-config service-name="skipserialization" group="SOAWebServiceClient-skipserialization">
        <service-interface-class-name>
			org.ebayopensource.turmeric.runtime.tests.service1.sample.services.message.Test1Service
		</service-interface-class-name>
        <service-location>
			http://localhost:9091/ws/spf
		</service-location>
        <wsdl-location>webservices/latest/eBaySvc.wsdl</wsdl-location>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="Local">
                	<override-options>
                		<skip-serialization>true</skip-serialization>
                	</override-options>
                </preferred-transport>
                <request-data-binding>XML</request-data-binding>
            </invocation-options>
            <pipeline-config/>
        </client-instance-config>
    </client-config>
    <client-config service-name="{http://www.ebayopensource.org/turmeric/common/config}test1gen" group="SOAWebServiceClient">
        <service-interface-class-name>
			org.ebayopensource.turmeric.runtime.tests.service1.sample.services.message.Test1Service
		</service-interface-class-name>
        <client-instance-config>
            <invocation-options>
                <preferred-transport name="LOCAL">
                    <override-options>
                        <num-connect-retries>3</num-connect-retries>
                    </override-options>
                </preferred-transport>
            </invocation-options>
        </client-instance-config>
    </client-config>
</client-config-list>
