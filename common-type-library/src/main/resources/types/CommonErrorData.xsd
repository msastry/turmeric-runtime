 <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.ebayopensource.org/turmeric/common/v1/types" xmlns:tns="http://www.ebayopensource.org/turmeric/common/v1/types" version="1.0.0">
    <xs:include schemaLocation="typelib://common-type-library//ErrorData.xsd"></xs:include>
    
    <xs:complexType name="CommonErrorData">
		<xs:annotation>
			<xs:documentation>Documentation goes here</xs:documentation>
			<xs:appinfo></xs:appinfo>
		</xs:annotation>
	
		<xs:complexContent>
			<xs:extension base="tns:ErrorData">
				<xs:sequence>
					<xs:element name="errorName" type="xs:string"></xs:element>
					<xs:element name="cause" type="xs:string" minOccurs="0"></xs:element>
					<xs:element name="resolution" type="xs:string" minOccurs="0"></xs:element>
					<xs:element name="errorGroups" type="xs:string" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Error group values are whitespace separated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="organization" type="xs:string"></xs:element>
		    			<xs:any maxOccurs="unbounded" minOccurs="0" processContents="lax"/>
			    	</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
</xs:schema>
